MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_FROM=
MAIL_FROM_NAME=

# Email Validation Service
ABSTRACT_EMAIL_URL=
ABSTRACT_EMAIL_API_KEY=

# SMS Configuration
TEXTBEE_API_BASE=
TEXTBEE_DEVICE_ID=
TEXTBEE_API_KEY=

# Payment Gateway
PAYMONGO_SECRET_KEY=

# Cache Configuration
CACHE_ENABLED=true
CACHE_DEFAULT_TTL=300