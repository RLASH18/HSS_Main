{
  "name": "hss_main",
  "version": "1.0.0",
  "scripts": {
    "build:css": "tailwindcss -i ./resources/css/app.css -o ./public/assets/css/style.css --minify",
    "watch:css": "tailwindcss -i ./resources/css/app.css -o ./public/assets/css/style.css --watch",
    "build:js": "esbuild resources/js/app.js --bundle --minify --outfile=public/assets/js/script.js",
    "watch:js": "esbuild resources/js/app.js --bundle --outfile=public/assets/js/script.js --watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.8",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "flowbite": "^3.1.2",
    "sweetalert2": "^11.22.2"
  }
}
